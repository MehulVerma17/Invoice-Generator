{
  "version": 2,
  "builds": [
    {
      "src": "src/custom_server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.{js,jsx,ts,tsx}",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/custom_server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
